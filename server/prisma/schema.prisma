// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Menu {
  id Int @id @default(autoincrement())
  image String @unique
  name String @unique
  type Type
  class Class @default(FOOD)
}
enum Class {
  FOOD
  DRINK
  OTHERS
}
enum Type {
  ITIK
  PORK
  BEEF
  CHICKEN
  SEAFOODS
  VEGETABLES
  DRINK
  OTHERS
}